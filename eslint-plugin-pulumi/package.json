{
  "name": "@raydak/eslint-plugin-pulumi",
  "version": "0.0.0-development",
  "description": "ESLint plugin with useful rules for Pulumi code.",
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "lint": "eslint",
    "test": "vitest"
  },
  "author": "raydak",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/utils": "8.46.1"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0",
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@pulumi/pulumi": "3.206.0",
    "@tsconfig/node-lts": "22.0.2",
    "@types/node": "24.10.0",
    "@typescript-eslint/rule-tester": "8.46.3",
    "eslint-plugin-eslint-plugin": "7.2.0",
    "eslint-plugin-n": "17.23.1",
    "tsdown": "0.16.1",
    "typescript-eslint": "8.46.3",
    "vitest": "3.2.4"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raydak-labs/eslint-plugin-pulumi.git",
    "directory": "eslint-plugin-pulumi"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "infrastructure",
    "lint",
    "pulumi"
  ]
}
